    "start": "npm run dev",
    "unit": "jest --config test/unit/jest.conf.js",
    "puppeteer": "jest --config test/puppeteer/jest.conf.js",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit",
    "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
    "move-map": "rm -rf ./sourcemap/js && mkdir ./sourcemap/js && mv ./public/static/js/*.map ./sourcemap/js",
    "build": "node build/build.js && npm run move-map",
    "deploy": "npm run build && firebase deploy",
    "deploy-hosting": "npm run build && firebase deploy --only hosting",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
